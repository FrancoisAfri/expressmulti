{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "albertcht/invisible-recaptcha": "^1.9",
        "ashallendesign/short-url": "^5.2",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "davmixcool/laravel-meta-manager": "^1.2",
        "elibyy/tcpdf-laravel": "^10.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "haruncpi/laravel-user-activity": "^1.0",
        "infinety/alerts": "*@dev",
        "intervention/image": "^2.7",
        "knplabs/knp-snappy": "^1.4",
        "laravel/framework": "^8.75",
        "laravel/helpers": "^1.6",
        "laravel/sanctum": "^2.15",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "mad-web/laravel-seoable": "^3.1",
        "owen-it/laravel-auditing": "^13.0",
        "plummer/calendarful": "^0.2.1",
        "ramsey/uuid-doctrine": "^1.8",
        "realrashid/sweet-alert": "^v3.1.6",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/data-transfer-object": "^2.8",
        "spatie/enum": "^3.12",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-permission": "^5.7",
        "spatie/pdf-to-text": "^1.52",
        "stancl/tenancy": "^3.6",
        "yajra/laravel-datatables-buttons": "^4.13",
        "yajra/laravel-datatables-oracle": "^9.21"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "st-man-hori/laravel-make-service": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
